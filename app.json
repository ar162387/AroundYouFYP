{
  "expo": {
    "name": "Around You",
    "slug": "around-you",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aroundyou.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Around You uses your location to show nearby shops and delivery estimates."
      },
      "config": {
        "googleMapsApiKey": "${EXPO_PUBLIC_GOOGLE_MAPS_API_KEY}"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3B82F6"
      },
      "package": "com.aroundyou.app",
      "config": {
        "googleMaps": {
          "apiKey": "${EXPO_PUBLIC_GOOGLE_MAPS_API_KEY}"
        }
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "react-native-maps"
    ],
    "extra": {
      "eas": {
        "projectId": "ea7a805e-d47f-42fa-a459-acb2b1a25514"
      }
    },
    "owner": "b3ast"
  }
}
